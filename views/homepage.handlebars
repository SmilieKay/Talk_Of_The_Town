<div class="container">
  {{#each blogs}}
    <div class="blog-post">
      <h2 class="blog-post-title">{{this.title}}</h2>
      <p class="blog-post-meta">Posted on: {{formatDate this.createdAt format="MM/DD/YYYY"}}</p>
      <p class="blog-post-content">{{this.content}}</p>
    </div>
  {{/each}}
</div>
